    repeat(abs(moveY)) {
        if (not place_meeting(x, y + sign(moveY), obj_obstacle_land)) {
            y += sign(moveY)
        }
    }
    jump_counter = 0
    moveY = 0
}

// Sprite Check
if (input_left + input_right != 0 and input_left + input_right != 2) {
    image_xscale = sign(input_right - input_left)
}

// Apply Movement
x += moveX
y += moveY
</string>
