switch (state) {
    case (player_states.idle):
        scr_player_idle();
        break;
    case (player_states.walk):
        scr_player_walk();
        break;
    case (player_states.jump):
        scr_player_jump();
        break;
    case (player_states.fly) :
        scr_player_fly();
        break;
}

